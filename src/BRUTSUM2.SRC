	ORG	$8000
BASE	RMB	2	STORE BASE ADDRESS OF 2K ROM
LOCA	RMB	2	STORE CHECKSUM DESTINATION
ROMN	RMB	1	STORE ROM NUMBER
CSUM	CLR	[LOCA]	CHECKSUM PROGRAM START ADDRESS
CSUMA	LDX	BASE		
	LDY	#$800
	LDA	ROMN
	CLRB
CSUM1	ADCA	,X+
	LEAY	-1,Y
	BNE	CSUM1
	CMPA	#$80
	BEQ	CSUM2
	INC	[LOCA]
	BNE	CSUMA
	BRA	*
CSUM2	JMP	[$FF16]
	END
